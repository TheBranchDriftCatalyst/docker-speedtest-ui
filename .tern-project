{
  "ecmaVersion": 7,
  "libs": [
    "browser"
  ],
  "loadEagerly": [
    "src/**/*.js",
    "tools/**/*.js"
  ],
  "dontLoad": ["node_modules/**"],
  "plugins": {
    "es_modules": {},
    "node": {},
    "doc_comment": {
      "fullDocs": false,
      "strong": true
    },
    "modules": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    }
  }
}
